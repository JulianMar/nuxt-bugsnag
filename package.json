{
  "name": "nuxt-bugsnag",
  "version": "8.2.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulianMar/nuxt-bugsnag.git"
  },
  "bugs": {
    "url": "https://github.com/JulianMar/nuxt-bugsnag/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "standard-version && git push --follow-tags && npm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@bugsnag/browser-performance": "^2.8.0",
    "@bugsnag/js": "^8.0.0",
    "@bugsnag/plugin-vue": "^8.0.0",
    "@bugsnag/vue-router-performance": "^2.8.0",
    "@nuxt/kit": "^3.4.3",
    "bugsnag-source-maps-fork": "^2.4.0",
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.14",
    "@nuxt/eslint-config": "^0.5.0",
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^3.4.3",
    "@nuxt/test-utils": "^3.4.3",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "nuxt": "^3.1.0",
    "standard-version": "^9.5.0",
    "vitest": "^2.0.0",
    "vue-tsc": "^2.0.29"
  }
}
